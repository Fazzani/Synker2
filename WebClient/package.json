  "scripts": {
    "test": "karma start ClientApp/test/karma.conf.js",
    "test:ci": "karma start ClientApp/test/karma.conf.js --log-level debug --single-run",
    "configure": "webpack --config webpack.config.vendor.js",
    "start:watch": "npm run build:dev && dotnet watch run",
    "build:dev": "webpack --env=dev --progress --profile --colors",
    "build:dist": "webpack --env=prod --progress --profile --colors",
    "prestart": "webpack --config webpack.config.vendor.js --progress --profile && dotnet build",
    "publish": "webpack --env=prod && dotnet publish",
    "deploy": "dotnet build && sh \"./Scripts/deploy.sh\""
  },
