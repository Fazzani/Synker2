{
  "SitePackIndex": "sitepack",
  "MediaRefIndex": "mediaref",
  "PiconIndex": "picons",
  "RequestTimeout": 2000,
  "Logging": {
    "Json": true,
    "PathFormat": "Logs/log-{Date}.log",
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
